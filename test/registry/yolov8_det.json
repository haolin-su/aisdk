{
    "model_name": "yolov8_det",
    "model_version": "1.0",
    "model_type": "object_detection",
    "model_format": "onnx",
    "model_path": "yolov8n.onnx",
    "model_inputs": [
        {
            "name": "images",
            "shape": [
                1,
                3,
                480,
                640
            ],
            "dtype": "float32",
            "format": "NCHW"
        }
    ],
    "model_outputs": [
        {
            "name": "output0",
            "shape": [
                1,
                84,
                6300
            ],
            "dtype": "float32",
        }
    ],
    "algroithm_data": {
        "preprocess": {
            "input_shape": [
                608,
                608
            ],
            "mean": [
                0,
                0,
                0
            ],
            "std": [
                1,
                1,
                1
            ]
        },
        "postprocess": {
            "conf_threshold": 0.5,
            "nms_threshold": 0.45,
            "iou_threshold": 0.5
        },
        "other_params": {
            // 这里可以加一下特殊的参数，比如label name list等
        }
    },
}